						number=$(shuf -i 0-$noOfFilesDE -n 1)
						check4=1
						clear
						openvpn	${TCP_de_array[$number]}
						echo -e "${RED}JOBBY${NC}"					


					elif [ $1 == 18 ] || [ $1 == "DENMARK" ] || [ $1 == "DK" ]; then

						number=$(shuf -i 0-$noOfFilesDK -n 1)
