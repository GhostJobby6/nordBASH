						check4=1
						clear
						openvpn	${TCP_cl_array[$number]}					


					elif [ $1 == 14 ] || [ $1 == "COSTA RICA" ] || [ $1 == "CR" ]; then

						number=$(shuf -i 0-$noOfFilesCR -n 1)
						check4=1
						clear
						openvpn	${TCP_cr_array[$number]}					


